<?xml version="1.0" encoding="UTF-8"?>
<ContentPage
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    Title="その他ページ"
    x:Class="check_to_travel_xamarin_forms.OtherPage">
    <ContentPage.Padding>
        <OnPlatform x:TypeArguments="Thickness" iOS="40,40,40,40" Android="40,40,40,40" />
    </ContentPage.Padding>
    <ContentPage.Content>
        <Grid x:Name="gridLayout">
            <Grid.RowDefinitions>
                <RowDefinition Height="100"/>
                <RowDefinition Height="100"/>
                <RowDefinition Height="100"/>
                <RowDefinition Height="100"/>
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="100"/>
                <ColumnDefinition Width="100"/>
                <ColumnDefinition Width="100"/>
            </Grid.ColumnDefinitions>
        </Grid>
    </ContentPage.Content>
</ContentPage>
